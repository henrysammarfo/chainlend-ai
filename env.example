# ChainLend AI Environment Configuration
# Copy this file to .env and fill in your values

# ZetaChain Configuration
ZETACHAIN_RPC_URL=https://zetachain-athens-evm.blockpi.network/v1/rpc/public
VITE_ZETACHAIN_RPC_URL=https://zetachain-athens-evm.blockpi.network/v1/rpc/public

# Smart Contract Deployment
PRIVATE_KEY=your_private_key_here
VITE_CONTRACT_ADDRESS=your_deployed_contract_address

# Google Gemini AI
VITE_GEMINI_API_KEY=your_gemini_api_key

# Wallet Connect (Optional)
VITE_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# Network Configuration
VITE_CHAIN_ID=7001
VITE_NETWORK_NAME=ZetaChain Athens Testnet

# API Keys (Optional - for additional features)
VITE_COINGECKO_API_KEY=your_coingecko_api_key
VITE_ETHERSCAN_API_KEY=your_etherscan_api_key

# Development
NODE_ENV=development
VITE_DEBUG_MODE=true
